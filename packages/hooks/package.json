{
   "name": "hooks",
   "version": "1.0.0",
   "exports": {
      "./query/*": {
         "types": "./src/query/*",
         "import": "./src/query/*",
         "require": "./dist/query/*"
      },
      "./*": {
         "types": "./src/*",
         "import": "./src/*",
         "require": "./dist/*"
      }
   },
   "scripts": {
      "watch_package": "tsup --watch"
   },
   "dependencies": {
      "configs": "*",
      "utils": "*",
      "assets": "*",
      "types": "*"
   },
   "devDependencies": {
      "tsup": "^8.4.0"
   },
   "peerDependencies": {
      "react-i18next": "*",
      "@tanstack/react-query": "*"
   }
}
